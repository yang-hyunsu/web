			"    <head>",
			"    <meta charset=\"UTF-8\">",
			"    <title>자바스크립트</title>",
			"    <script>",
			"        /*",
			"        # $1 ",
			"        */",
			"        ",
			"    </script>",
			"</head>",
			"<body>",
			"    <h2>타이틀</h2>",
			"$2",
			"</body>",
			"    <script>",
			"        /*",
			"        # $3 ",
			"        */",
			"        ",
			"    </script>",			
			"</html>"
